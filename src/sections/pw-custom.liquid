{% include 'pw-login' with {
  headline: section.settings.headline,
  bg_image: section.settings.background-image
} %}
  
{% schema %}
  {
    "name": "Password Signup",
    "settings": [
      {
        "id": "headline",
        "type": "text",
        "label": "Headline",
        "default": "Welcome from Texas!"
      },
      {
        "id": "background-image",
        "type": "image_picker",
        "label": "Background Image"
      }
    ]
  }
{% endschema %}